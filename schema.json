components:
  schemas:
    BlogPost:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        title:
          type: string
          maxLength: 200
        slug:
          type: string
          maxLength: 50
          pattern: ^[-a-zA-Z0-9_]+$
        summary:
          type: string
        content:
          type: string
        content_format:
          type: string
          maxLength: 20
        language:
          type: string
          maxLength: 10
        status:
          type: string
          description: draft|published|archived
          maxLength: 20
        published_at:
          type: string
          format: date-time
          readOnly: true
        updated_at:
          type: string
          format: date-time
          readOnly: true
        excerpt:
          type: string
        table_of_contents: {}
        reading_time:
          type: integer
          maximum: 32767
          minimum: 0
        tags: {}
        views_count:
          type: integer
          maximum: 2147483647
          minimum: 0
        likes_count:
          type: integer
          maximum: 2147483647
          minimum: 0
        bookmarks_count:
          type: integer
          maximum: 2147483647
          minimum: 0
        comments_count:
          type: integer
          maximum: 2147483647
          minimum: 0
        allow_comments:
          type: boolean
        featured:
          type: boolean
        pinned_order:
          type: integer
          maximum: 32767
          minimum: -32768
        seo_title:
          type: string
          maxLength: 200
        seo_description:
          type: string
          maxLength: 300
        canonical_url:
          type: string
          format: uri
          maxLength: 200
        og_image_url:
          type: string
          format: uri
          maxLength: 200
        cover_image:
          type: string
          format: uri
          nullable: true
        cover_image_url:
          type: string
          format: uri
          maxLength: 200
        series:
          type: integer
          nullable: true
      required:
      - id
      - published_at
      - slug
      - title
      - updated_at
    BlogSeries:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        title:
          type: string
          maxLength: 200
        slug:
          type: string
          maxLength: 50
          pattern: ^[-a-zA-Z0-9_]+$
        description:
          type: string
        order:
          type: integer
          maximum: 32767
          minimum: -32768
        published:
          type: boolean
        cover_image:
          type: string
          format: uri
          nullable: true
        cover_image_url:
          type: string
          format: uri
          maxLength: 200
      required:
      - id
      - slug
      - title
    BlogSubscription:
      type: object
      properties:
        email:
          type: string
          format: email
          maxLength: 254
        verified:
          type: boolean
          readOnly: true
        active:
          type: boolean
          readOnly: true
      required:
      - active
      - email
      - verified
    ChatAsk:
      type: object
      properties:
        provider:
          $ref: '#/components/schemas/ProviderEnum'
        model:
          type: string
          maxLength: 100
        question:
          type: string
          maxLength: 4000
        max_tokens:
          type: integer
        structured:
          type: boolean
          default: true
        top_n:
          type: integer
          maximum: 20
          minimum: 1
          default: 6
      required:
      - provider
      - question
    ChatLog:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        provider:
          type: string
          maxLength: 50
        model:
          type: string
          maxLength: 100
        question:
          type: string
        answer:
          type: string
        answer_json:
          nullable: true
        status:
          type: string
          maxLength: 20
        error:
          type: string
        tokens_prompt:
          type: integer
          maximum: 2147483647
          minimum: -2147483648
          nullable: true
        tokens_completion:
          type: integer
          maximum: 2147483647
          minimum: -2147483648
          nullable: true
        latency_ms:
          type: integer
          maximum: 2147483647
          minimum: -2147483648
          nullable: true
        created_at:
          type: string
          format: date-time
          readOnly: true
      required:
      - created_at
      - id
      - provider
      - question
    Contact:
      type: object
      properties:
        name:
          type: string
          maxLength: 200
        email:
          type: string
          format: email
        message:
          type: string
          maxLength: 2000
      required:
      - email
      - message
      - name
    Experience:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        company:
          type: string
          maxLength: 200
        role:
          type: string
          maxLength: 200
        start_date:
          type: string
          format: date
        end_date:
          type: string
          format: date
          nullable: true
        description:
          type: string
        location:
          type: string
          maxLength: 200
        employment_type:
